				<alias font_style="italic" label_side="right" label_angle="45" uid="629" x="1878.33" y="307.351" width="18" height="18">
					<of>&quot;Max._recoverable_gravel&quot;</of>
				</alias>
				<alias font_style="italic" label_side="top" uid="649" x="2009.83" y="348.208" width="18" height="18">
					<of>Maximal_gravel_extraction</of>
				</alias>
				<alias font_style="italic" font_weight="bold" uid="657" x="1536.83" y="1032.67" width="18" height="18">
					<of>Local_primary_gravel_demand</of>
				</alias>
				<alias font_style="italic" label_side="left" uid="665" x="202.5" y="937.253" width="18" height="18">
					<of>Expected_gravel_from_extraction</of>
				</alias>
				<alias font_style="italic" uid="670" x="1751" y="906" width="18" height="18">
					<of>Relative_attractiveness_of_extraction_and_disposal_to_recovery_from_excavation</of>
				</alias>
				<alias font_style="italic" label_side="left" uid="676" x="202.5" y="898.583" width="18" height="18">
					<of>Initial_potential_gravel_quarries</of>
				</alias>
				<alias color="black" background="white" font_style="italic" label_side="right" label_angle="315" uid="678" x="319.25" y="860.291" width="18" height="18">
					<shape type="circle" radius="18"/>
					<of>Initial_geological_reserves</of>
				</alias>
				<alias font_style="italic" label_side="left" uid="705" x="787.429" y="396.333" width="18" height="18">
