			<view isee:show_pages="false" background="white" page_width="1093" page_height="763" isee:page_cols="3" isee:page_rows="2" isee:scroll_x="86" isee:scroll_y="307" isee:popup_graphs_are_comparative="false" isee:enable_non_negative_highlights="false" type="stock_flow">
