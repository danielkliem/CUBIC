	<model name="Settlement">
		<variables>
			<aux name="Aggregate demand" access="output">
				<doc>The gravel demand in Region A, as a result of construction activity. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>(Reference_value_gravel_demand*Conversion_t_to_m3)*Change_aggregate_demand</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>(Reference_value_gravel_demand*Conversion_t_to_m3)</eqn>
				</element>
				<units>t/year</units>
			</aux>
			<aux name="Excavation Material" access="output" isee:autocreated="true">
				<doc>The amount of excavation material in Region A, as a result of construction activity. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>(Reference_Excavation_Material*Conversion_t_to_m3)*(Change_excavation_material)</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>((Reference_Excavation_Material[Region_B]*Conversion_t_to_m3))</eqn>
				</element>
				<units>t/year</units>
			</aux>
			<aux name="CDW" access="output" isee:autocreated="true">
				<doc>The amount of CDW in Region A, as a result of construction activity. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>(Reference_value_CDW[Region_A]*Conversion_t_to_m3)*Change_CDW[Region_A]</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>(Reference_value_CDW[Region_B]*Conversion_t_to_m3)</eqn>
				</element>
				<units>t/year</units>
			</aux>
			<aux name="Period 1 gravel">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>1+(RAMP(Period_1_change_Aggregates/Duration_ramp_period_1,Starting_time_Period_1, Starting_time_Period_1+Duration_ramp_period_1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Period 1 change Aggregates">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>&quot;%_change_of_gravel_Period_1&quot;/100</eqn>
				<format delimit_000s="false"/>
				<range min="0" max="10"/>
				<units>1</units>
			</aux>
			<aux name="% change of gravel Period 1">
				<doc>Change of gravel demand in Region A in %, relative to equilibrium. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>IF Greenfield_development=1
THEN 30
ELSE IF Urban_renewal=1
THEN 30
ELSE IF Urban_densification=1 
THEN -30
ELSE IF Reduction=1 
THEN -70
ELSE &quot;Region_A:_%_change_of_gravel&quot;</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>IF Greenfield_development=1
THEN 30.6079
ELSE IF Urban_renewal=1
THEN 10
ELSE IF Urban_densification=1 
THEN -30
ELSE IF Reduction=1 
THEN -70
ELSE &quot;Region_B:_%_change_of_gravel&quot;</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="-99" max="70"/>
				<units>1</units>
			</aux>
			<aux name="Period 1 change\nExcavation Material">
				<doc>The ramp height is divided by 100 to provide input for the ramp. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>&quot;%_change_of_excavation_material_Period_1&quot;/100</eqn>
				<format delimit_000s="false"/>
				<range min="0" max="10"/>
				<units>1</units>
			</aux>
			<aux name="% change of excavation\nmaterial Period 1">
				<doc>Change of excavation Material in Region A in %, relative to equilibrium. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>IF Greenfield_development=1
THEN 2
ELSE IF Urban_renewal=1
THEN 15
ELSE IF Urban_densification=1 
THEN -30
ELSE IF Reduction=1 
THEN -70
ELSE &quot;Region_A:_%_change_of_excavation_material&quot;</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>IF Greenfield_development=1
THEN 2
ELSE IF Urban_renewal=1
THEN 30
ELSE IF Urban_densification=1 
THEN -60
ELSE IF Reduction=1 
THEN -70
ELSE &quot;Region_B:_%_change_of_excavation_material&quot;</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="-99" max="70"/>
				<units>1</units>
			</aux>
			<aux name="Period 1 CDW">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>1+(RAMP(Period_1_CDW_change/Duration_ramp_period_1,Starting_time_Period_1, Starting_time_Period_1+Duration_ramp_period_1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Period 1 CDW change">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>&quot;%_change_of_CDW_Period_1&quot;/100</eqn>
				<format delimit_000s="false"/>
				<range min="0" max="10"/>
				<units>1</units>
			</aux>
			<aux name="% change of CDW Period 1">
				<doc>Change of CDW in Region A in %, relative to equilibrium. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>IF Greenfield_development=1
THEN 10
ELSE IF Urban_renewal=1
THEN 40
ELSE IF Urban_densification=1 
THEN 30
ELSE IF Reduction=1 
THEN 50
ELSE &quot;Region_A:_%_change_of_CDW&quot;</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>IF Greenfield_development=1
THEN 10
ELSE IF Urban_renewal=1
THEN 40
ELSE IF Urban_densification=1 
THEN 30
ELSE IF Reduction=1 
THEN 50
ELSE &quot;Region_B:_%_change_of_CDW&quot;</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="-99" max="70"/>
				<units>1</units>
			</aux>
			<aux name="Reference value gravel\ndemand">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>3500000</eqn>
				<units>m3/year</units>
			</aux>
			<aux name="Reference Excavation\nMaterial">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Initial_Delta</eqn>
				<units>m3/year</units>
			</aux>
			<aux name="Reference value CDW">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>500000</eqn>
				<units>m3/year</units>
			</aux>
			<aux name="Delta material\nnet-flow">
				<doc>The delta of material flows indicates whether the available disposal volume is increasing or shrinking (assuming 100 % recycling of CDW) . </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>(Aggregate_demand-(CDW+Excavation_Material))/Conversion_t_to_m3</eqn>
				<units>m3/year</units>
			</aux>
			<stock name="Developed area">
				<doc>The area of Region A that is part of the building stock and provides residency to the population. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Initial_developed_area</eqn>
				<inflow>Replacement</inflow>
				<inflow>Greenfield_construction</inflow>
				<outflow>Deconstruction</outflow>
				<non_negative/>
				<units>ha</units>
			</stock>
			<flow name="Replacement">
				<doc>Assumes that the deconstructed area in Region A is replaced at an equal rate. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Deconstruction</eqn>
				<non_negative/>
				<units>ha/year</units>
			</flow>
			<flow name="Deconstruction">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Developed_area*Deconstruction_rate</eqn>
				<non_negative/>
				<units>ha/year</units>
			</flow>
			<stock name="Undeveloped area">
				<doc>The area of Region A for greenfield construction. It is assumed that regardless of population growth, that area will be built on.</doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Initial_undeveloped_area</eqn>
				<inflow>building_land_release</inflow>
				<outflow>Greenfield_construction</outflow>
				<units>ha</units>
			</stock>
			<flow name="Greenfield\nconstruction">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Undeveloped_area/Reference_construction_time</eqn>
				<non_negative/>
				<units>ha/year</units>
			</flow>
			<aux name="Deconstruction rate">
				<doc>A constant rate at which the developed area is deconstructed. </doc>
				<eqn>0.02</eqn>
				<units>1/year</units>
			</aux>
			<aux name="relative developed area">
				<doc>Assumes that developed area is directly transformed into floor space. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>(Developed_area/(Undeveloped_area+Total_area))</eqn>
				<units>1</units>
			</aux>
			<aux name="Sensitivity of settlement pressure">
				<doc>Assumes a high sensitivity, to reflect the growing NIMBY phenomena in urban and developing areas. </doc>
				<eqn>2</eqn>
				<units>1</units>
			</aux>
			<aux name="Normalized Settlement pressure">
				<doc>The Settlement pressure is used to indicate the population density relative to available space within the region. If more new land for construction is made available, the settlement pressure increases. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>(Settlement_pressure/Initial_settlement_pressure)^Sensitivity_of_settlement_pressure</eqn>
				<units>1</units>
			</aux>
			<aux name="Initial undeveloped\narea" access="output" isee:autocreated="true">
				<doc>The initial undeveloped are in Region A equals the intial floor space minus minus the fraction of undeveloped area. </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Initial_total_area* Fraction_of_undeveloped</eqn>
				<units>ha</units>
			</aux>
			<aux name="Initial developed area">
				<doc>The initial floor space in Region A equals the inital demand for floor space </doc>
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<eqn>Initial_undeveloped_area*Fraction_of_developed_area</eqn>
				<units>ha</units>
			</aux>
			<aux name="Greenfield\ndevelopment" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>Switch_Region_A_Greenfield_development</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>Switch_Region_B_Greenfield_development</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="0" max="1"/>
				<units>1</units>
			</aux>
			<aux name="Urban renewal" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>Switch_Region_A_Urban_renewal</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>Switch_Region_B_Urban_renewal</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="0" max="1"/>
				<units>1</units>
			</aux>
			<aux name="Urban densification" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>Switch_Region_A_Urban_densification</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>Switch_Region_B_Urban_densification</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="0" max="1"/>
				<units>1</units>
			</aux>
			<aux name="Reduction" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Region"/>
				</dimensions>
				<element subscript="Region_A">
					<eqn>Switch_Region_A_Reduction</eqn>
				</element>
				<element subscript="Region_B">
					<eqn>Switch_Region_B_Reduction</eqn>
				</element>
				<format precision="1" delimit_000s="false"/>
				<range min="0" max="1"/>
