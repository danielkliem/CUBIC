					<of>&quot;Starting_Time_Policy:_Minimum_gravel_supply&quot;</of>
				</alias>
				<alias font_style="italic" label_side="bottom" uid="715" x="1122.91" y="1159.83" width="18" height="18">
					<of>&quot;Max._recoverable_gravel&quot;</of>
				</alias>
				<alias font_style="italic" label_side="left" uid="791" x="189" y="752.25" width="45" height="35">
					<of>Potential_gravel_quarries</of>
				</alias>
				<alias font_style="italic" label_side="left" uid="793" x="184" y="818.291" width="45" height="35">
